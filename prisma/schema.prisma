generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Equipament {
  id        String   @id @default(uuid())
  series    String
  model     String
  mark      String
  status    Int?
  createdAt DateTime @default(now())
}

model User {
  id       String  @id @default(uuid())
  name     String
  login    String?
  senha    String?
  email    String
  position String
}
